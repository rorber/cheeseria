{
  "name": "@cheeseria/api",
  "version": "0.0.0",
  "main": "dist/services/core-api/src/index.js",
  "types": "dist/services/core-api/src/index.d.ts",
  "scripts": {
    "dev": "npm-run-all --parallel watch nodemon alias",
    "dev:server": "TZ=UTC serverless offline --httpPort 4321 --stage offline",
    "nodemon": "nodemon",
    "alias": "tsc-alias -w",
    "watch": "tsc --watch",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && pnpm create:json:stores",
    "build:ci": "rm -rf dist && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && pnpm create:json:stores:ci",
    "create:json:stores": "cp src/routes/cheese/cheeses.json dist/routes/cheese/cheeses.json && cp src/routes/order/orders.json dist/routes/order/orders.json",
    "create:json:stores:ci": "cp src/routes/cheese/cheeses.json dist/services/core-api/src/routes/cheese/cheeses.json && cp src/routes/order/orders.json dist/services/core-api/src/routes/order/orders.json",
    "clean": "rm -rf .build .turbo dist node_modules types"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.425.0",
    "@aws-sdk/client-secrets-manager": "^3.425.0",
    "@aws-sdk/client-sfn": "^3.425.0",
    "@cheeseria/domain": "workspace:^0.0.0",
    "@trpc/server": "^10.14.0",
    "date-fns": "^2.30.0",
    "dotenv-json": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "trpc-panel": "^1.3.4",
    "winston": "^3.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.111",
    "@types/eslint": "^8.21.3",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.11.26",
    "@types/serverless": "^3.12.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.11",
    "eslint": "^8.35.0",
    "jest": "^29.4.3",
    "jest-mock-extended": "3.0.4",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "serverless": "^3.28.1",
    "serverless-offline": "^12.0.4",
    "serverless-offline-ssm": "^6.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  }
}
